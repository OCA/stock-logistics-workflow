<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <record id="stock_picking_type_lend" model='stock.picking.type'>
        <field name="name">Lends</field>
        <field name="code">internal</field>
        <field name="show_reserved" eval="True"/>
        <field name="default_location_src_id" ref="stock.stock_location_stock"/>
        <field name="default_location_dest_id" ref="stock_items_lend.stock_lend_location"/>
        <field name="sequence_id" ref="stock_items_lend.stock_lend_sequence"/>
        <field name="sequence" eval="12"/>
     </record>
      <record id="stock_picking_type_lend_return" model='stock.picking.type'>
        <field name="name">Lend Returns</field>
        <field name="code">internal</field>
        <field name="show_reserved" eval="True"/>
        <field name="default_location_src_id" ref="stock_items_lend.stock_lend_location"/>
        <field name="default_location_dest_id" ref="stock.stock_location_stock"/>
        <field name="sequence_id" ref="stock_items_lend.stock_lend_return_sequence"/>
        <field name="sequence" eval="13"/>
     </record>
</odoo>