<odoo>
    <record id="demo_car_rate_1" model="delivery.carrier.rate">
        <field name="date_generated">2017-01-01</field>
        <field name="rate" eval="10.00"/>
        <field name="retail_rate" eval="15.00"/>
        <field name="rate_currency_id" ref="base.USD"/>
        <field name="sale_order_id" ref="demo_order_1"/>
        <field name="service_id" ref="demo_carrier_1"/>
    </record>

    <record id="demo_car_rate_2" model="delivery.carrier.rate">
        <field name="date_generated">2017-01-02</field>
        <field name="rate" eval="15.00"/>
        <field name="retail_rate" eval="20.00"/>
        <field name="rate_currency_id" ref="base.USD"/>
        <field name="sale_order_id" ref="demo_order_2"/>
        <field name="service_id" ref="demo_carrier_1"/>
    </record>
</odoo>
